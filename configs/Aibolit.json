{
    "model": {
        "name": "models.aibolit.densenet_dropout.CNN",
        "hparams": {
            "optimizer": {
                "type": "adam",
                "params": {
                    "lr": 0.0007557584138874836,
                    "weight_decay": 0.0003969897923152853
                }
            },
            "train_batch_size": 32,
            "eval_batch_size": 100,
            "growth_rate": 8,
            "compression": 0.3,
            "kernel_size": 7,
            "dropout": 0,
            "bottleneck": false
        }
    },
    "dataset": {
        "dataset_generator": "datasets.common.h5_dataset_generator.H5DatasetGenerator",
        "examples_provider": "datasets.common.h5_examples_provider.H5ExamplesProvider",
        "params": {
            "example_duration": 4,
            "seed": 100,
            "example_fs": 500,
            "class_settings": {
                "N": {
                    "label_map": 0,
                    "equalize_distribution": 1.4
                },
                "R": {
                    "label_map": 1,
                    "equalize_distribution": false
                },
                "AFIB": {
                    "label_map": 2,
                    "equalize_distribution": false
                },
                "L": {
                    "label_map": 3,
                    "equalize_distribution": false
                },
                "AV": {
                    "label_map": 4,
                    "equalize_distribution": 1
                },
                "AR": {
                    "label_map": 5,
                    "equalize_distribution": 1
                },
                "PVC": {
                    "label_map": 6,
                    "equalize_distribution": false
                },
                "CLC": {
                    "label_map": 7,
                    "equalize_distribution": false
                }
            },
            "split_ratio": [
                0.8,
                0.2
            ],
            "test_set_size": 0,
            "normalize_input": true
        },
        "sources": [
            {
                "name": "aibolit//dataset-standard",
                "examples_generator": "datasets.aibolit.cardian_examples_generator.CardianExamplesGenerator",
                "fs": 1000,
                "class_settings": {
                    "CLC": {
                        "rhythm": "clc",
                        "overlap": 0.75
                    },
                    "PVC": {
                        "rhythm": "pvc",
                        "overlap": 0.75
                    },
                    "R": {
                        "rhythm": "r",
                        "overlap": 0.7
                    },
                    "AFIB": {
                        "rhythm": "afib",
                        "overlap": 0.5
                    },
                    "L": {
                        "rhythm": "l",
                        "overlap": 0.5
                    },
                    "AV": {
                        "rhythm": "av",
                        "overlap": 0.5
                    },
                    "AR": {
                        "rhythm": "ar",
                        "overlap": 0
                    },
                    "N": {
                        "rhythm": "n",
                        "overlap": 0
                    }
                }
            }
        ]
    },
    "max_epochs": 60,
    "keep_n_checkpoints": 4,
    "early_stopper_params": {
        "patience": 8
    },
    "eval_scheduler_params": {
        "interval": 3,
        "decrease_step": 1,
        "decrease_timeout": 15
    },
    "lr_scheduler_params": {
        "factor": 0.2,
        "patience": 4
    },
    "params_space": "configs.hyperopt_spaces.aibolit_densenet.Space",
    "iteration": "5"
}