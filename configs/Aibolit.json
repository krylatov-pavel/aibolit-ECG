{
    "model": {
        "name": "models.aibolit.cnn_5layer.CNN",
        "experiment": "aibolit_8class_5layer_2fold_h5",
        "hparams": {
            "optimizer": {
                "type": "adam",
                "params": {
                    "lr": 0.005393,
                    "weight_decay": 0.00016
                }
            },
            "dropout": 0.3973,
            "train_batch_size": 32,
            "eval_batch_size": 100,
            "filters_num": 60,
            "fc_units": 45,
            "fc_layers": 3,
            "activation_fn": "torch.nn.ReLU"
        }
    },
    "dataset": {
        "dataset_generator": "datasets.common.h5_dataset_generator.H5DatasetGenerator",
        "examples_provider": "datasets.common.h5_examples_provider.H5ExamplesProvider",
        "params": {
            "example_duration": 3.3,
            "example_fs": 250,
            "class_settings": {
                "N": {
                    "label_map": 0,
                    "equalize_distribution": 1.4
                },
                "R": {
                    "label_map": 1,
                    "equalize_distribution": false
                },
                "AFIB": {
                    "label_map": 2,
                    "equalize_distribution": false
                },
                "L": {
                    "label_map": 3,
                    "equalize_distribution": false
                },
                "AV": {
                    "label_map": 4,
                    "equalize_distribution": 1
                },
                "AR": {
                    "label_map": 5,
                    "equalize_distribution": 1
                },
                "PVC": {
                    "label_map": 6,
                    "equalize_distribution": false
                },
                "CLC": {
                    "label_map": 7,
                    "equalize_distribution": false
                }
            },
            "split_ratio": [
                0.8,
                0.2
            ],
            "test_set_size": 0,
            "normalize_input": false
        },
        "sources": [{
            "name": "aibolit",
            "examples_generator": "datasets.aibolit.cardian_examples_generator.CardianExamplesGenerator",
            "fs": 1000,
            "resample_fs": 250,
            "class_settings": {
                "CLC": {
                    "rhythm": "clc",
                    "overlap": 0.71
                },
                "PVC": {
                    "rhythm": "pvc",
                    "overlap": 0.71
                },
                "R": {
                    "rhythm": "r",
                    "overlap": 0.662
                },
                "AFIB": {
                    "rhythm": "afib",
                    "overlap": 0.493
                },
                "L": {
                    "rhythm": "l",
                    "overlap": 0.493
                },
                "AV": {
                    "rhythm": "av",
                    "overlap": 0.493
                },
                "AR": {
                    "rhythm": "ar",
                    "overlap": 0
                },
                "N": {
                    "rhythm": "n",
                    "overlap": 0
                }
            }
        }]
    },
    "max_epochs": 50,
    "keep_n_checkpoints": 4,
    "early_stopper_params": {
        "patience": 8
    },
    "eval_scheduler_params": {
        "interval": 3,
        "decrease_step": 1,
        "decrease_timeout": 15
    },
    "lr_scheduler_params": {
        "factor": 0.2,
        "patience": 4
    },
    "params_space": "configs.hyperopt_spaces.aibolit_5layer.Space",
    "iteration": "default"
}