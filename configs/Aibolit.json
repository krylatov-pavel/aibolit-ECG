{
    "model": {
        "name": "models.aibolit.cnn_5layer.CNN",
        "experiment": "aibolit_7class_5layer_2fold_pvc",
        "hparams": {
            "optimizer": {
                "type": "adam",
                "params": {
                    "lr": 0.005393,
                    "weight_decay": 0.00016
                }
            },
            "dropout": 0.3973,
            "train_batch_size": 32,
            "eval_batch_size": 100,
            "filters_num": 60,
            "fc_units": 45,
            "fc_layers": 3,
            "activation_fn": "torch.nn.ReLU"
        }
    },
    "dataset": {
        "dataset_provider": "datasets.aibolit.file_dataset_provider.FileDatasetProvider",
        "file_provider": "datasets.common.wavedata_provider.WavedataProvider",
        "params": {
            "source_name": "aibolit",
            "bypass_cache": false,
            "example_duration": 3.3,
            "fs": 1000,
            "resample_fs": 250,
            "class_settings": [
                {
                    "name": "PVC",
                    "rhythm": "pvc",
                    "overlap": 0.662,
                    "equalize_distribution": false
                },
                {
                    "name": "R",
                    "rhythm": "r",
                    "overlap": 0.662,
                    "equalize_distribution": false
                },
                {
                    "name": "AFIB",
                    "rhythm": "afib",
                    "overlap": 0.594,
                    "equalize_distribution": false
                },
                {
                    "name": "L",
                    "rhythm": "l",
                    "overlap": 0.594,
                    "equalize_distribution": 1
                },
                {
                    "name": "AV",
                    "rhythm": "av",
                    "overlap": 0.493,
                    "equalize_distribution": 1
                },
                {
                    "name": "AR",
                    "rhythm": "ar",
                    "overlap": 0,
                    "equalize_distribution": 1
                },
                {
                    "name": "N",
                    "rhythm": "n",
                    "overlap": 0,
                    "equalize_distribution": 1.5
                }
            ],
            "label_map": {
                "N": 0,
                "R": 1,
                "L": 2,
                "AV": 3,
                "AR": 4,
                "AFIB": 5,
                "PVC": 6
            },
            "split_ratio": [
                0.8,
                0.2
            ],
            "test_set_size": 0,
            "normalize_input": false
        }
    },
    "max_epochs": 220,
    "keep_n_checkpoints": 4,
    "early_stopper_params": {
        "patience": 7
    },
    "eval_scheduler_params": {
        "interval": 3,
        "decrease_step": 1,
        "decrease_timeout": 12
    },
    "lr_scheduler_params": {
        "factor": 0.2,
        "patience": 4
    },
    "wait_improvement_n_evals": 4,
    "params_space": "configs.hyperopt_spaces.aibolit_5layer.Space",
    "iteration": "default"
}