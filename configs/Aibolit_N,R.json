{
    "model": {
        "name": "models.aibolit.cnn_generic.CNN",
        "experiment": "aibolit_N_R_generic",
        "max_epochs": 220,
        "hparams": {
            "learning_rate": 0.001,
            "dropout_rate": 0.5,
            "train_batch_size": 32,
            "eval_batch_size": 100,
            "filters_num": 64,
            "filters_step": 32,
            "conv_kernel_size": 5,
            "conv_layer_num": 3,
            "dropout": 0.4,
            "fc_units": 128
        }
    },
    "dataset" : {
        "provider" : "datasets.aibolit.examples_provider.ExamplesProvider",
        "params": {
            "db_name": "aibolit",
            "experiment": "4-fold_(N,R)_250hz",
            "bypass_cache": false,
            "example_duration": 3,
            "fs": 1000,
            "resample_fs": 250,
            "rhythm_filter": [{
                "name": "N",
                "rhythm": "N",
                "overlap": 0.2,
                "distribution": 2.5
            
            }, {
                "name": "R",
                "rhythm": "R",
                "overlap": 0.6,
                "take_last": true,
                "distribution": 1
            }],
            "label_map": {
                "N": 0,
                "R": 1
            },
            "rhythm_map": {},
            "normalize": true,
            "split_ratio": [0.25, 0.25, 0.25, 0.25],
            "test_set_size": 0.0,
            "include_augmented": false
        }
    },
    "max_to_keep": 20,
    "params_space": "configs.hyperopt_spaces.arrhythmia_(N,B,SBR,AFIB).Space",
    "iteration": "default"
}