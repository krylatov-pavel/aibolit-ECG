{
    "model": {
        "name": "models.aibolit.cnn_generic.CNN",
        "experiment": "aibolit_N_R_AV_generic_4fold",
        "max_epochs": 220,
        "hparams": {
            "learning_rate": 0.00033,
            "dropout": 0.55,
            "train_batch_size": 32,
            "eval_batch_size": 100,
            "filters_num": 48,
            "filters_step": 32,
            "conv_kernel_size": 7,
            "conv_layer_num": 7,
            "fc_units": 40
        }
    },
    "dataset" : {
        "dataset_provider" : "datasets.aibolit.file_dataset_provider.FileDatasetProvider",
        "file_provider" : "datasets.common.wavedata_provider.WavedataProvider",
        "params": {
            "source_name": "aibolit",
            "bypass_cache": false,
            "example_duration": 3,
            "fs": 1000,
            "resample_fs": 250,
            "class_settings": [{
                "name": "N",
                "rhythm": "N",
                "overlap": 0,
                "equalize_distribution": 2
            
            }, {
                "name": "R",
                "rhythm": "R",
                "overlap": 0.5,
                "take_last": true,
                "equalize_distribution": 1
            }, {
                "name": "AV",
                "rhythm": "AV",
                "overlap": 0.5,
                "take_last": true,
                "equalize_distribution": false
            }],
            "label_map": {
                "N": 0,
                "R": 1,
                "AV": 2
            },
            "rhythm_map": {},
            "normalize": true,
            "split_ratio": [0.25, 0.25, 0.25, 0.25],
            "test_set_size": 0.05,
            "include_augmented": false
        }
    },
    "max_to_keep": 20,
    "params_space": "configs.hyperopt_spaces.aibolit_(N,R).Space",
    "iteration": "default"
}